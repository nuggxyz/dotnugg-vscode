{
    "name": "dotnugg",
    "displayName": "dotnugg",
    "description": "A VS Code extension for working with dotnugg files",
    "version": "0.1.2",
    "author": "dub6ix <dub6ix@nugg.xyz>",
    "private": false,
    "license": "MIT",
    "engines": {
        "vscode": "^1.61.0"
    },
    "repository": "https://github.com/nuggxyz/dotnugg-vscode.git",
    "icon": "icon.png",
    "publisher": "nuggxyz",
    "categories": [
        "Other",
        "Programming Languages"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [],
        "languages": [
            {
                "id": "dotnugg",
                "aliases": [
                    "dotnugg"
                ],
                "extensions": [
                    ".nugg"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "dotnugg",
                "scopeName": "source.dotnugg",
                "path": "./node_modules/@nuggxyz/dotnugg-grammar/dotnugg.tmLanguage.json"
            }
        ]
    },
    "extensionKind": [
        "workspace"
    ],
    "scripts": {
        "build": "tsc -p ./",
        "deploy": "vsce publish --yarn",
        "watch": "tsc -watch -p ./",
        "watchts": "yarn ts-node -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "yarn run compile && node ./out/test/script.js",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "test-compile": "tsc -p ./"
    },
    "devDependencies": {
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "@types/vscode": "^1.61.0",
        "@typescript-eslint/eslint-plugin": "^4.31.1",
        "@typescript-eslint/parser": "^4.31.1",
        "@vscode/test-electron": "^1.6.2",
        "arraybuffer-loader": "^1.0.8",
        "esbuild-plugin-wasm": "^1.0.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-universe": "^7.0.1",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-unused-imports": "^1.1.4",
        "file-loader": "^6.2.0",
        "mocha": "^9.1.1",
        "path-browserify": "^1.0.1",
        "prettier": "2.4.1",
        "ts-loader": "^9.2.6",
        "typescript": "^4.4.3",
        "vsce": "^1.100.2",
        "wasm-loader": "^1.3.0",
        "webpack": "^5.68.0",
        "webpack-cli": "^4.9.2"
    },
    "dependencies": {
        "@nuggxyz/dotnugg-sdk": "^0.1.10",
        "tiny-invariant": "^1.1.0",
        "vscode-textmate": "^5.4.0"
    }
}
